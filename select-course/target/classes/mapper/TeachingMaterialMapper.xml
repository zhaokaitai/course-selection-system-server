<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.heub.selectcourse.mapper.TeachingMaterialMapper">

    <resultMap id="BaseResultMap" type="com.heub.selectcourse.model.domain.TeachingMaterial">
            <id property="bookCode" column="book_code" jdbcType="VARCHAR"/>
            <result property="bookName" column="book_name" jdbcType="VARCHAR"/>
            <result property="bookAuthor" column="book_author" jdbcType="VARCHAR"/>
            <result property="press" column="press" jdbcType="VARCHAR"/>
            <result property="versionNumber" column="version_number" jdbcType="VARCHAR"/>
            <result property="isbn" column="ISBN" jdbcType="VARCHAR"/>
            <result property="publicationDate" column="publication_date" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        book_code,book_name,book_author,
        press,version_number,ISBN,
        publication_date
    </sql>
</mapper>
